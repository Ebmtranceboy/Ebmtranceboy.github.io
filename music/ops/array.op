opcode row,i[],i[]iii
  iarr[],irow,icol,ir xin 
  
  icopy[] init icol
  iy = -1
  loop:
  	iy += 1
    icopy[iy] = iarr[ir][iy]
  	if iy < icol -1 goto loop
  xout icopy
endop 

opcode transpact,i[],i[]iii
  iarr[],ir1,irn,ic xin 
  
  itrp[][] init ic, irn - ir1 + 1
  
  ix = -1
  loopx:
  	ix += 1
  	iy = -1
  	loopy:
  		iy += 1
  		itrp[ix][iy] = iarr[(ir1+iy)*ic+ix]
  		if iy < irn - ir1 goto loopy
	if ix < ic - 1 goto loopx
  xout itrp
endop 
